{
    "databases": {
        "path_based": {
            "type": "path_based_db",
            "connection_string": "path/to/path/database",
            "max_connections": 10,
            "timeout": 30
        },
        "arc_based": {
            "type": "arc_based_db",
            "connection_string": "path/to/arc/database",
            "max_connections": 5,
            "timeout": 30
        }
    },
    "initialization": {
        "order": ["path_data", "arc_data"],
        "retry_count": 3,
        "retry_delay": 1000
    },
    "logging": {
        "level": "INFO",
        "file": "data_access.log",
        "max_size": "10MB"
    }
}
